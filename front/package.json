{
  "name": "azviewer-webapp-prototype",
  "version": "0.0.1",
  "description": "AZviewer(prototype) Web App Project",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --colors --hot --config ./webpack.config.dev.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/setup.js 'src/**/*spec.js'",
    "test:watch": "npm run test -- --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "build-prod": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress",
    "start-prod": "cross-env NODE_ENV=production node server.js",
    "start-daemon": "cross-env NODE_ENV=production node_modules/forever/bin/forever start server.js",
    "stop-daemon": "cross-env NODE_ENV=production node_modules/forever/bin/forever stop server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
