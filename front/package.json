    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.1",
    "exports-loader": "^0.6.4",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
